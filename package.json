{
    "name": "@agentic/triage",
    "version": "0.3.0",
    "description": "AI-powered GitHub issue triage, PR review, and sprint planning - Vercel AI SDK primitives",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "agentic-triage": "./dist/cli.js",
        "triage": "./dist/cli.js"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./schemas": {
            "types": "./dist/schemas/index.d.ts",
            "import": "./dist/schemas/index.js"
        },
        "./tools": {
            "types": "./dist/tools/index.d.ts",
            "import": "./dist/tools/index.js"
        },
        "./handlers": {
            "types": "./dist/handlers/index.d.ts",
            "import": "./dist/handlers/index.js"
        },
        "./reporters/vitest": {
            "types": "./dist/reporters/vitest.d.ts",
            "import": "./dist/reporters/vitest.js"
        },
        "./reporters/playwright": {
            "types": "./dist/reporters/playwright.d.ts",
            "import": "./dist/reporters/playwright.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "clean": "rimraf dist",
        "typecheck": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "lint": "biome lint .",
        "lint:fix": "biome lint --write .",
        "format": "biome format --write .",
        "format:check": "biome format .",
        "check": "biome check .",
        "check:fix": "biome check --write .",
        "docs": "typedoc",
        "prepare": "husky",
        "semantic-release": "semantic-release"
    },
    "keywords": [
        "triage",
        "ai",
        "automation",
        "agentic",
        "ollama",
        "anthropic",
        "vercel-ai-sdk",
        "mcp",
        "playwright",
        "testing",
        "security",
        "codeql",
        "sarif",
        "github",
        "octokit",
        "cli",
        "sprint-planning",
        "release-management",
        "issue-triage",
        "code-review"
    ],
    "author": "Jon Bogaty <jon@jonbogaty.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/agentic-dev-library/triage.git"
    },
    "homepage": "https://github.com/agentic-dev-library/triage#readme",
    "bugs": {
        "url": "https://github.com/agentic-dev-library/triage/issues"
    },
	"engines": {
		"node": ">=20"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
    "dependencies": {
        "@actions/core": "^2.0.1",
        "@ai-sdk/mcp": "^1.0.1",
        "@linear/sdk": "^68.1.0",
        "@modelcontextprotocol/sdk": "^1.25.1",
        "@playwright/mcp": "latest",
        "ai": "^6.0.0",
        "ai-sdk-ollama": "^3.0.0",
        "commander": "^14.0.2",
        "glob": "^13.0.0",
        "octokit": "^5.0.5",
        "ollama": "^0.6.3",
        "picocolors": "^1.1.1",
        "zod": "^4.2.1"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.3.10",
        "@playwright/test": "^1.57.0",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^13.0.1",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^12.0.2",
        "@semantic-release/npm": "^13.1.3",
        "@semantic-release/release-notes-generator": "^14.1.0",
        "@types/node": "^25.0.3",
        "@vitest/coverage-v8": "^3.2.4",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.7",
        "rimraf": "^6.1.2",
        "semantic-release": "^25.0.2",
        "tsx": "^4.21.0",
        "typedoc": "^0.28.15",
        "typescript": "^5.9.3",
        "vitest": "^3.2.4"
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx,json}": [
            "biome check --write --no-errors-on-unmatched"
        ]
    }
}
